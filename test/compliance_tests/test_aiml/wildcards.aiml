</category>

<category><pattern>* STARTEST</pattern>
<template>*star = <star/></template>
</category>
	
<category><pattern>UNDERSCORETEST _</pattern>
<template>_star = <star/></template>
</category>

<category><pattern>_ UNDERSCORETEST</pattern>
<template>_star = <star/></template>
</category>

